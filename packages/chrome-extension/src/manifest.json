{
  "manifest_version": 3,
  "version": "0.1.0",
  "name": "Perfsee",
  "description": "The Chrome extension for Perfsee platform.",
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "alarms",
    "background",
    "clipboardWrite",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "options_page": "options.html",
  "minimum_chrome_version": "112.0.5569.0",
  "icons": {
    "128": "favicon.ico"
  }
}
